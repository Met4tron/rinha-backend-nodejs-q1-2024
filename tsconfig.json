{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "ES2021",
    "sourceMap": false,
    "esModuleInterop": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ],
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}